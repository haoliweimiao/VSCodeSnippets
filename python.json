	"create new python file": {
		"prefix":"python.new",
		"body":[
			"#!/usr/bin/env python\n",
			"def test():",
			"    $1",
			"    return\n",
			"if __name__ == \"__main__\":",
			"    test()"
		]
	},
	"import local path to env":{
		"prefix": "python.add.local.path",
		"body": [
		"import sys",
		"sys.path.append(\"./\")\n"	
		]
	}
